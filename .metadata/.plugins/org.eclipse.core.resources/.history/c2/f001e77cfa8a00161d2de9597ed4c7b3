package qwq;

import com.opensymphony.xwork2.ActionSupport;

public class SearchClass extends ActionSupport {
	private String name;
	
	public String getauthorsName() {
		return name;
	}
	
	public void setauthorsName(String name) {
		this.name = name;
	}
	
	public String execute() {
		name = "Here is the books written by" + name + "!";
		return "SUCCESS";
	}
}
